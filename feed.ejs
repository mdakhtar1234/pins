<% include ./partials/header.ejs%>

  <div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
    <div class="card  flex flex-wrap gap-10">
      <% posts.forEach(function(elem){ %>

        <div class="card w-32">
          <div class="w-full h-40  rounded-lg overflow-hidden">
            <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
          </div>
          <h5 class="mt-1 text-sm"><%= elem.title %><h5>
            <h6 class="text-xs opacity-50 font-semibold">@<%= elem.username %></h6>
        </div>
        <% }) %>

    </div>
  </div>
  <% include ./partials/footer.ejs%>